    </action>
  </item>
  <item label="dwb (Win+变换)">
    <action name="Execute">
      <command>dwb</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
    </item>
    <item label="Claws Mail">
    <action name="Execute">
      <command>claws-mail</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

</menu>

<menu id="apps-office-menu" label="Dev">
