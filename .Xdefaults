! URxvt ------------------------------------
URxvt*saveLines:   1500
URxvt*termName: rxvt-unicode

!URxvt*font:xft:Envy Code R:size=10:regular,xft:WenQuanYi Bitmap Song:size=9:regular
!URxvt*boldFont:xft:Envy Code R:size=10:bold,xft:WenQuanYi Bitmap Song:size=9:bold
!URxvt*italicFont:xft:Envy Code R:size=10:italic,xft:WenQuanYi Bitmap Song:size=9:italic
!URxvt.font:              xft:DejaVu LGC Sans Mono:size=10:book
!URxvt.boldFont:          xft:DejaVu LGC Sans Mono:size=10:bold
!URxvt.italicFont:        xft:DejaVu LGC Sans Mono:size=10:oblique

URxvt.font:       [codeset=ASCII]xft:DejaVu Sans Mono for Powerline:size=8,\
                  [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:regular
URxvt.boldFont:   [codeset=ASCII]xft:DejaVu Sans Mono for Powerline:bold:size=8,\
                  [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:bold
URxvt.italicFont: [codeset=ASCII]xft:DejaVu Sans Mono for Powerline:italic:size=8,\
                  [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:italic

URxvt.keysym.M-Up: command: \
        \033]710;[codeset=ASCII]xft:DejaVu Sans Mono for Powerline:size=8, [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:regular:size=8\007 \
        \033]711;[codeset=ASCII]xft:DejaVu Sans Mono for Powerline:bold:size=8, [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:bold:size=8\007 \
        \033]712;[codeset=ASCII]xft:DejaVu Sans Mono for Powerline:italic:size=8, [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:italic:size=8\007
URxvt.keysym.M-Down: command: \
        \033]710;[codeset=ASCII]xft:DejaVu Sans Mono for Powerline:size=8, [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:regular\007 \
        \033]711;[codeset=ASCII]xft:DejaVu Sans Mono for Powerline:bold:size=8, [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:bold\007 \
        \033]712;[codeset=ASCII]xft:DejaVu Sans Mono for Powerline:italic:size=8, [codeset=ISO10646]xft:WenQuanYi Micro Hei Mono:italic\007

! URxvt.font:       [codeset=ASCII]xft:Envy Code R for Powerline:size=8,\
!                   [codeset=ISO10646]xft:WenQuanYi Bitmap Song:regular
! URxvt.boldFont:   [codeset=ASCII]xft:Envy Code R for Powerline:bold:size=8,\
!                   [codeset=ISO10646]xft:WenQuanYi Bitmap Song:bold
! URxvt.italicFont: [codeset=ASCII]xft:Envy Code R for Powerline:italic:size=8,\
!                   [codeset=ISO10646]xft:WenQuanYi Bitmap Song:italic
! 
! URxvt.keysym.M-Up: command: \
!         \033]710;[codeset=ASCII]xft:Envy Code R for Powerline:size=8, [codeset=ISO10646]xft:WenQuanYi Bitmap Song:regular:size=8\007 \
!         \033]711;[codeset=ASCII]xft:Envy Code R for Powerline:bold:size=8, [codeset=ISO10646]xft:WenQuanYi Bitmap Song:bold:size=8\007 \
!         \033]712;[codeset=ASCII]xft:Envy Code R for Powerline:italic:size=8, [codeset=ISO10646]xft:WenQuanYi Bitmap Song:italic:size=8\007
! URxvt.keysym.M-Down: command: \
