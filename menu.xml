      </startupnotify>
    </action>
  </item>
</menu>

<menu id="openbox-menu" label="Configurations">
  <item label="Appearance Manager">
    <action name="Execute">
      <command>lxappearance</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <separator />
  <item label="Openbox Configuration Manager">
    <action name="Execute">
      <command>obconf</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <separator />
  <item label="Reconfigure Openbox">
    <action name="Reconfigure" />
  </item>
</menu>

<menu id="apps-misc-menu" label="Miscellaneous">
  <item label="Evince">
    <action name="Execute">
      <command>evince</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <separator />
  <item label="Calculator">
    <action name="Execute">
      <command>galculator</command>
    </action>
  </item>
</menu>

<menu id="root-menu" label="Openbox 3">
  <separator label="Start" />
    <item label="Terminal">
      <action name="Execute">
        <startupnotify>
          <enabled>yes</enabled>
          <name>Terminal</name>
          <icon></icon>
        </startupnotify>
        <command>terminator --geometry=960x660+30+25</command>
      </action>
    </item>
  <separator label="Applications" />
  <menu id="apps-accessories-menu"/>
  <menu id="apps-editors-menu"/>
  <menu id="apps-graphics-menu"/>
  <menu id="apps-net-menu"/>
  <menu id="apps-office-menu"/>
  <menu id="apps-multimedia-menu"/>
  <menu id="apps-term-menu"/>
  <menu id="apps-fileman-menu"/>
  <menu id="apps-misc-menu"/>
  <separator label="System" />
  <menu id="openbox-menu"/>
  <menu id="system-menu"/>
  <separator />
  <menu id="system-menu" label="System">
    <item label="Log Out">
      <action name="Exit">
        <prompt>yes</prompt>
		<!-- <command>pkill &#45;KILL python python2 wbar tint2 volumeicon synergys mpd openbox</command> -->
		<command>openbox --exit</command>
      </action>
    </item>
    <separator />
    <item label="Reboot">
      <action name="Execute">
        <prompt>Are you sure you want to reboot?</prompt>
        <command>systemctl reboot</command>
      </action>
    </item>
    <separator />
    <item label="Poweroff">
      <action name="Execute">
        <prompt>Are you sure you want to shut down Archlinux?</prompt>
		<command>systemctl poweroff</command>
      </action>
    </item>
  </menu>

</menu>

</openbox_menu>
