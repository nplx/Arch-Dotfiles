!!urxvt*geometry:                  124x53+5+30
urxvt*geometry:                  90x30
! urxvt*inputMethod:               ibus
urxvt*inputMethod:               fcitx
! urxvt*depth:                     32
! urxvt*background:                rgba:0000/0000/0200/c800
! urxvt*background:                #182235
urxvt*background:                #0e1726
urxvt*borderLess:                false
urxvt*scrollBar:                 false
urxvt*matcher.button:            1
urxvt*cursorBlink:               true
!urxvt*cursorColor:               #258aef
urxvt*cursorColor:               Green
urxvt*allow_bold:                true
urxvt*internalBorder:            2
! urxvt*inheritPixmap:             true
!!urxvt*tintColor:                 #
! urxvt*shading:                   100
! urxvt*transparent:               true
! urxvt*shading:                   30
!!urxvt*fading:                    0
!!urxvt*fadeColor:                 #222222
urxvt*urgentOnBell:              true

!!------------Clickable------------------
URxvt.perl-ext-common: default,matcher
URxvt*urlLauncher: /usr/bin/dwb
!!URxvt.matcher.button: 2
URxvt*matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
!! Colors

!!------------xterm----------------
!look and feel
xterm*termName: xterm-256color
xterm*geometry: 72x32
xterm*scrollBar: false
xterm*rightScrollBar: true
xterm*loginshell: true
xterm*cursorBlink: true
! xterm*reverseVideo: on
xterm*background:  black
! xterm*foreground:  grey
! xterm*foreground:  green
xterm*cursorColor:  green
xterm*borderLess: true
xterm*activeIcon: true
xterm*colorUL: yellow
! xterm*colorBD: white

!input & fix alt key
! xterm*inputMethod: ibus
xterm*inputMethod: fcitx
!xterm*eightBitInput: true
!xterm*altSendsEscape: true

!font and locale
xterm*locale: true
xterm*utf8:     true
xterm*utf8Title: true
xterm*fontMenu*fontdefault*Label: Default
! xterm*faceName: DejaVu Sans Mono for Powerline:antialias=True:pixelsize=11
! xterm*boldFont: DejaVu Sans Mono for Powerline:style=Bold:pixelsize=11
! xterm*faceNameDoublesize: YaHei Consolas Hybrid:antialias=True:pixelsize=11
! xterm*faceName: Terminess Powerline:pixelsize=9
! xterm*boldFont: Terminess Powerline:pixelsize=9
xterm*faceName: DejaVu Sans Mono for Powerline:pixelsize=11
xterm*boldFont: DejaVu Sans Mono for Powerline:pixelsize=11
xterm*faceNameDoublesize: WenQuanYi Bitmap Song:pixelsize=12
xterm*xftAntialias: true
!xterm*cjkWidth:true
!xterm*preeditType:Root

!------------ Emacs settings ------------
! Emacs*useXIM: false
! Emacs*geometry: 120x35
Emacs.cursorColor: yellow
Emacs*borderWidth: 0
Emacs*internalBorder: 0
!Emacs.background: black
!Emacs.foreground: white
Emacs.pointerColor: yellow
Emacs*borderColor: black
!Emacs*mode-line*attributeBackground: black
!Emacs*mode-line*attributeForeground: white

! xscreensaver ---------------------------------------------------------------
!font settings
xscreensaver.Dialog.headingFont:        -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.bodyFont:           -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.labelFont:          -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.unameFont:          -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.buttonFont:         -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.dateFont:           -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.passwd.passwdFont:         -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #111111
xscreensaver.Dialog.background:         #ffffff
xscreensaver.Dialog.topShadowColor:     #111111
xscreensaver.Dialog.bottomShadowColor:  #111111
xscreensaver.Dialog.Button.foreground:  #ffffff
xscreensaver.Dialog.Button.background:  #666666
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #ffffff
xscreensaver.Dialog.text.background:    #666666
xscreensaver.Dialog.internalBorderWidth:24
xscreensaver.Dialog.borderWidth:        20
xscreensaver.Dialog.shadowThickness:    2
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #ff0000
xscreensaver.passwd.thermometer.background:  #000000
xscreensaver.passwd.thermometer.width:       8
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %b %d
