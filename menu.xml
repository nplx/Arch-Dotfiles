<menu id="openbox-menu" label="Configurations">
  <item label="Appearance Manager">
    <action name="Execute">
      <command>lxappearance</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <separator />
  <item label="Openbox Configuration Manager">
    <action name="Execute">
      <command>obconf</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <separator />
  <item label="Reconfigure Openbox">
    <action name="Reconfigure" />
  </item>
</menu>

<menu id="root-menu" label="Openbox 3">
  <separator label="Start" />
    <item label="Terminal">
      <action name="Execute">
        <startupnotify>
          <enabled>yes</enabled>
          <name>Terminal</name>
          <icon></icon>
        </startupnotify>
        <command>xterm</command>
      </action>
    </item>
    <item label="Eclipse">
      <action name="Execute">
        <startupnotify>
          <enabled>yes</enabled>
          <name>Eclipse</name>
          <icon></icon>
        </startupnotify>
        <command>eclipse</command>
      </action>
    </item>
  <separator label="Applications" />
  <menu id="apps-accessories-menu"/>
  <menu id="apps-editors-menu"/>
  <menu id="apps-graphics-menu"/>
  <menu id="apps-net-menu"/>
  <menu id="apps-office-menu"/>
  <menu id="apps-multimedia-menu"/>
  <menu id="apps-term-menu"/>
  <menu id="apps-fileman-menu"/>
  <separator label="System" />
  <menu id="openbox-menu"/>
  <menu id="system-menu"/>
  <separator />
  <menu id="system-menu" label="System">
    <item label="Log Out">
      <action name="Exit">
        <prompt>yes</prompt>
      </action>
    </item>
    <separator />
    <item label="Reboot">
      <action name="Execute">
        <prompt>Are you sure you want to reboot?</prompt>
        <command>sudo shutdown -r now</command>
      </action>
    </item>
    <separator />
    <item label="Poweroff">
      <action name="Execute">
        <prompt>Are you sure you want to shut down Archlinux?</prompt>
        <command>sudo shutdown -h now</command>
      </action>
    </item>
  </menu>

</menu>

</openbox_menu>
