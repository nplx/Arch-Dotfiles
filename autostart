#xfce4-power-manager &
#nm-applet &

#Set xcompmgr-dana
xcompmgr -fF -t-5 -l-5 -r4.2 -o.55 -D2 -m.83 &

#Run transset
transset-df .70 -n Conky &

#Run Panel
tint2 &

#Run volume tray icon
volumeicon &

#Run clipit
clipit &

